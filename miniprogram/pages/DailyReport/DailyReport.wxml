<!--pages/DailyReport/DailyReport.wxml-->
<topbar curIndex='19'/>
<view class="user_tab">
  <view class="Name">{{name}}</view>
  <view class="Time">{{date}} {{week}}</view>
</view>

<view class="daily_report">
  <view class="group">
    <view class="report">应出勤0.0天，实际出勤0.0天</view>
  </view>
  <view class="group">
    <view class="report">有薪假共0.0h，无薪假共0.0h</view>
  </view>
  <view class="group">
    <view class="report">平常加班：0.0h</view>
    <view class="report">休日加班：0.0h</view>
    <view class="report">假日加班：0.0h</view>
  </view>
  <view class="group">
    <view class="report">迟到时长：0.00m</view>
    <view class="report">早退时长：0m</view>
    <view class="report">缺勤时长：0.00h</view>
    <view class="report">旷工时长：0.00h</view>
    <view class="report">缺卡次数：0次</view>
  </view>
  <view class="group">
    <view class="report">班次：无班次</view>
    <view class="report">签到：14:43:18  签退：14:49:51</view>
  </view>
</view>